import java.util.*;
import java.io.*;
class Main3 {
	public static void main(String[] args) throws Exception{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int t = Integer.parseInt(br.readLine());
		while( t--> 0)
		{
			boolean ug = true;
			String first = br.readLine();
			int n = Character.getNumericValue(first.charAt(0)); int nn = n;
			int m = Character.getNumericValue(first.charAt(2));
			
			if(n == 0) ug = false;
			else if (m == 0) ug = true;
			else
			{
				//int[] count = new int[m];
				int[] edges = new int[m];
				Arrays.fill(edges, -1);
				boolean[] connections = new boolean[n];
				while(n --> 0)
				{
					int currentV = nn-n-1;
					String[] row = br.readLine().split(" ");
					for (int i = 0; i < m && ug; i++) 
					{
						if(row[i].charAt(0) == '1')
						{
							if(edges[i] == -1) edges[i] = currentV;
							else if(edges[i] == -2) ug = false;
							else
							{
								if(connections[edges[i]]) ug = false;
								else
								{
									connections[i] = true;
									edges[i] = -2;
								}
							}
						}
					}
				}
			}
			String resp = (ug)?"Yes":"No";
			System.out.println(resp);
		}
		
	}
}