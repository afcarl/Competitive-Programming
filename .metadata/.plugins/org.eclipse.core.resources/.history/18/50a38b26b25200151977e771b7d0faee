import java.util.Arrays;


public class Solution {
    
    int[] number2 = {};
    int[] number5 = {};
    
	public int trailingZeroes(int a) {

	    number2 = new int[a+1];
	    number5 = new int[a+1];
	    Arrays.fill(number2, -1);
	    Arrays.fill(number5, -1);
	    int sum2 =0;
	    int sum5 = 0;
	    
	    for(int i = 0; i <= a; i++)
	    {
	        sum2+=numberTwo(a);
            sum5+=numberFive(a);
	    }
	    return Math.min(sum2, sum5);
	}
	
	public int numberTwo(int a)
	{
	    if(a < 2) return 0;
	    else
	    {
	        if(number2[a] != -1) return number2[a];
	        
	        number2[a]= 0;
	        if(a % 2 == 0) number2[a] +=numberTwo(a/2)+1;
	        return number2[a];
	    }
	}
	
	public int numberFive(int a)
	{
	    if(a < 5) return 0;
	    else
	    {
	        if(number5[a] != -1) return number5[a];
	        number5[a] = 0;
	        if(a % 5 == 0) number5[a] += numberFive(a/5)+1;
	        return number5[a];
	    }
	}

	public static void main(String[] args) {
		
		Solution x = new Solution();
		System.out.println(x.trailingZeroes(5));

	}
	
	
	

}
